{% extends 'base.html.twig' %}

{% block title %}Categorie
{% endblock %}

{% block body %}

	<div class="container py-4">
		<h2 class="text-center text-white bg-primary col-md-7 col-lg-7 py-4 mx-auto my-2">Categorie</h2>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ categorie.id }}</td>
				</tr>
				<tr>
					<th>Nom</th>
					<td>{{ categorie.nom }}</td>
				</tr>
				<tr>
					<th>Slug</th>
					<td>{{ categorie.slug }}</td>
				</tr>
			</tbody>
		</table>

		<div class="text-center">
			<a class="btn btn-sm btn-primary" href="{{ path('app_admin_cat_index') }}">Retour</a>
		</div>
		
	</div>
{% endblock %}
